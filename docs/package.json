{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "yarn export",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "export": "vite build && node ace export && copyfiles -u 1 public/* public/**/* build"
  },
  "imports": {
    "#exceptions/*": "./app/exceptions/*.js",
    "#generated/*": "./.adonisjs/server/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#collections/*": "./app/collections/*.js",
    "#spark/*": "./app/spark/*.jsx",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^8.0.0-next.26",
    "@adonisjs/eslint-config": "^3.0.0-next.5",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^2.0.0-next.3",
    "@japa/assert": "^4.2.0",
    "@japa/browser-client": "^2.2.0",
    "@japa/plugin-adonisjs": "^5.1.0-next.0",
    "@japa/runner": "^5.0.0",
    "@npmcli/package-json": "^7.0.4",
    "@poppinss/ts-exec": "^1.4.1",
    "@radix-ui/colors": "^3.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/alpinejs": "^3.13.11",
    "@types/copyfiles": "^2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.3",
    "alpinejs": "^3.15.3",
    "copyfiles": "^2.4.1",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "hot-hook": "^0.4.1-next.1",
    "pino-pretty": "^13.1.3",
    "pkg-types": "^2.3.0",
    "prettier": "^3.7.4",
    "ts-node-maintained": "^10.9.6",
    "typescript": "~5.9",
    "vite": "^7.3.0",
    "youch": "^4.1.0-beta.13"
  },
  "dependencies": {
    "@adonisjs/content": "^1.5.0",
    "@adonisjs/core": "^7.0.0-next.16",
    "@adonisjs/static": "^2.0.0-next.1",
    "@adonisjs/vite": "^5.1.0-next.2",
    "@fontsource/google-sans-flex": "^5.2.1",
    "@fontsource/instrument-sans": "^5.2.8",
    "@mdx-js/mdx": "^3.1.1",
    "@shikijs/rehype": "^3.20.0",
    "@sparkjs/adonis": "workspace:*",
    "@sparkjs/spark": "workspace:*",
    "@sparkjs/sparkles": "workspace:*",
    "@sparkjs/unpoly": "workspace:*",
    "@tailwindcss/vite": "^4.1.18",
    "@vinejs/vine": "^4.2.0",
    "better-sqlite3": "^12.5.0",
    "clsx": "^2.1.1",
    "edge.js": "^6.4.0",
    "globby": "^16.1.0",
    "luxon": "^3.7.2",
    "reflect-metadata": "^0.2.2",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vfile-matter": "^5.0.1"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
